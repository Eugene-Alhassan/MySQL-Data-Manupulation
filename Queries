# Query 1
SELECT 
    *
FROM
    employees
WHERE
    FIRST_NAME Like ('Mark%');
    
# Query 2
SELECT 
    *
FROM
    employees
WHERE
    hire_date LIKE ('%2000%'); 
    
# Query 3
SELECT 
    *
FROM
    employees
WHERE
    emp_no LIKE ('1000_'); 

# Query 4
SELECT 
    *
FROM
    salaries
WHERE
    salary BETWEEN 66000 AND 70000; 
    
# Query 5
SELECT 
    *
FROM
    employees
WHERE
    emp_no NOT BETWEEN '10004' AND '10012';
    
# Query 6  
SELECT 
    *
FROM
    departments
WHERE
    dept_no BETWEEN 'd003' AND 'd006';
    
   #Query 7
   SELECT 
    e.emp_no, e.first_name, e.last_name, e.hire_date, dm.dept_no
FROM
    employees e
        JOIN
    dept_manager dm ON e.emp_no = dm.emp_no;
